<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/appBackGround"
    tools:context=".Fragment.FeedBackListFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/newHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/demoLayout"
        android:layout_margin="5dp"
        android:paddingLeft="5dp"
        android:text="Feed Back List"
        android:drawableRight="@drawable/feedback"
        android:drawablePadding="50dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/red"
        android:textStyle="bold" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewForFeedBack"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/newHeader"
        tools:itemCount="9"
        tools:listitem="@layout/feedback_list" />

</RelativeLayout>